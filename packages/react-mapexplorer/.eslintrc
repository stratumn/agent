{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "env": {
		"browser": true,
    "node": true
  },
  "plugins": [
	  "react",
		"jsx"
  ],
  "rules": {
    "comma-dangle": 0,
    "no-console": 0,
    "no-param-reassign": [2, {
      "props": false
    }],
    "react/jsx-uses-vars": [2],
    "import/no-unresolved": ["error", { "ignore": ["^react$"] }]
  }
}
