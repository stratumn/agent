{
  "name": "stratumn-agent",
  "version": "0.18.3",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "test":
      "mocha --compilers js:babel-core/register --recursive --require should",
    "test:ci":
      "babel-node ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- --recursive --require should",
    "build:lib": "babel src --out-dir lib",
    "clean": "rimraf lib coverage",
    "preversion": "npm run clean && npm run test",
    "version": "npm run build:lib",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build:lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stratumn/agent-js.git"
  },
  "keywords": ["stratumn", "sdk", "blockchain", "client"],
  "author": "Stratumn Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stratumn/agent-js/issues"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.15.2",
    "canonical-json": "0.0.4",
    "cors": "^2.7.1",
    "express": "^4.14.0",
    "qs": "^6.5.1",
    "request": "^2.73.0",
    "superagent": "^2.1.0",
    "uuid": "^3.0.0",
    "ws": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-plugin-transform-runtime": "latest",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015-node4": "latest",
    "babel-preset-es2017": "^6.9.0",
    "isparta": "^4.0.0",
    "istanbul": "0.4.4",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.2",
    "rimraf": "^2.5.3",
    "should": "^8.2.2",
    "superagent-mocker": "^0.4.0",
    "supertest": "^1.2.0"
  }
}
