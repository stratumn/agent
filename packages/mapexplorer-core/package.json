{
  "name": "mapexplorer-core",
  "version": "1.2.0",
  "description": "Core library for building Map Explorer components",
  "main": "lib/mapexplorer-core.js",
  "module": "lib/mapexplorer-core.mjs",
  "jsnext:main": "lib/mapexplorer-core.mjs",
  "scripts": {
    "test:unit": "mocha --compilers js:babel-register  --require should test/unit/*",
    "test:integration": "karma start karma.conf.js",
    "test:integration:ci": "npm run test:integration -- --single-run",
    "test": "npm run test:unit && npm run test:integration",
    "test:ci": "npm run test:unit && npm run test:integration:ci",
    "prebuild": "eslint src test",
    "build": "rollup -c rollup.es.config.js && rollup -c rollup.umd.config.js && rollup -c rollup.browser.config.js && rollup -c rollup.browser.min.config.js",
    "prepublishOnly": "npm run build",
    "postpublish": "aws s3 cp dist/mapexplorer-core.js s3://stratumn-libs/mapexplorer-core.js --region 'eu-west-1' && aws s3 cp dist/mapexplorer-core.min.js s3://stratumn-libs/mapexplorer-core.min.js --region 'eu-west-1' && aws s3 cp dist/mapexplorer-core.js s3://stratumn-libs/mapexplorer-core-${npm_package_version}.js --region 'eu-west-1' && aws s3 cp dist/mapexplorer-core.min.js s3://stratumn-libs/mapexplorer-core-${npm_package_version}.min.js --region 'eu-west-1'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stratumn/indigo-js.git"
  },
  "keywords": [
    "stratumn",
    "blockchain",
    "mapexplorer",
    "chainscript"
  ],
  "author": "Stratumn Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stratumn/indigo-js/issues"
  },
  "homepage": "https://github.com/stratumn/indigo-js",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^6.0.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.11.6",
    "babelrc-rollup": "^3.0.0",
    "crypto-browserify": "^3.11.0",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.1.0",
    "karma": "^1.1.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-rollup-plugin": "^0.2.2",
    "karma-should": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "rollup": "^0.34.7",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-coffee-script": "^1.1.0",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-builtins": "^1.0.7",
    "rollup-plugin-node-globals": "^1.0.6",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^2.5.0",
    "should": "^8.2.2",
    "xhr-mock": "^1.7.0",
    "zock": "^0.2.6"
  },
  "dependencies": {
    "canonical-json": "0.0.4",
    "d3-array": "^1.0.0",
    "d3-ease": "^1.0.0",
    "d3-hierarchy": "^1.0.0",
    "d3-selection": "^1.0.0",
    "d3-transition": "^1.0.1",
    "d3-zoom": "^1.0.2",
    "deepmerge": "^0.2.10",
    "httpplease": "^0.16.4",
    "js-sha256": "^0.3.0",
    "stratumn-agent-client": "^1.9.0"
  }
}
